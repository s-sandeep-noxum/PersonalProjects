<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:CommandImages="clr-namespace:IconsLibrary" >
    <SolidColorBrush x:Key="DisabledMask" Color="#44000000"/>

    <Style x:Key="CommandImageStyle" TargetType="Image">
        <!--<Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="RenderOptions.BitmapScalingMode" Value="NearestNeighbor"/>-->
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
    </Style>


    <Style TargetType="{x:Type CommandImages:Computer01}" BasedOn="{StaticResource CommandImageStyle}">
        <Setter Property="Source" Value="pack://application:,,,/IconsLibrary;component/UI-48/Computer.png"/>
    </Style>

    <Style TargetType="{x:Type CommandImages:Computer02}" BasedOn="{StaticResource CommandImageStyle}">
        <Setter Property="Source" Value="pack://application:,,,/IconsLibrary;component/UI-48/ComputerNew.png"/>
    </Style>

    <Style TargetType="{x:Type CommandImages:Project}" BasedOn="{StaticResource CommandImageStyle}">
        <Setter Property="Source" Value="pack://application:,,,/IconsLibrary;component/UI-48/Project.png"/>
    </Style>
</ResourceDictionary>